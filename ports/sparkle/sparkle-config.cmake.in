@PACKAGE_INIT@

set_and_check(SPARKLE_FRAMEWORK "@PACKAGE_SPARKLE_FRAMEWORK@")
set_and_check(SPARKLE_INCLUDE_DIR "@PACKAGE_SPARKLE_INCLUDE_DIR@")
set_and_check(SPARKLE_LIBRARY "@PACKAGE_SPARKLE_FRAMEWORK@/Sparkle")

add_library(Sparkle::Sparkle INTERFACE IMPORTED)
set_target_properties(Sparkle::Sparkle PROPERTIES
    INTERFACE_INCLUDE_DIRECTORIES "${SPARKLE_INCLUDE_DIR}"
    INTERFACE_LINK_LIBRARIES "${SPARKLE_LIBRARY}"
)
